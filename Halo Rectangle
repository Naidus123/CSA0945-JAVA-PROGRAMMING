public class Main {
    public static void main(String[] args) {
        int rows = 6;
        int cols = 10; 
        
        for (int i = 0; i < rows; i++) {
            for (int j = 0; j < cols; j++) {
                
                if (i == 0 || i == rows - 1 || j == 0 || j == cols - 1) {
                    System.out.print("x ");
                } else {
                    System.out.print("  "); 
                }
            }
            System.out.println();
        }
    }
}
